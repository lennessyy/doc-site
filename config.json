{
  "index_name": "tgdocs",
  "start_urls": [
    {
      "url": "https://docs.tigergraph.com/(?P<component>.*?)/(?P<version>.*?)/",
      "variables": {
        "component": [
          "tigergraph-server", "gsql-ref", "gui"
        ],
        "version": [
          "3.2",
          "3.3",
          "3.4",
          "3.5"
        ]
      }
    },
    {
      "url": "https://docs.tigergraph.com/(?P<component>.*?)/current/",
      "variables": {
        "component": [
          "tigergraph-server", "gsql-ref", "gui"
        ]
      },
      "extra_attributes": {
        "version": "3.6"
      }
    },
    {
      "url": "https://docs.tigergraph.com/(?P<component>.*?)/current/",
      "variables": {
        "component": [
          "graph-ml"
        ]
      },
      "extra_attributes":{
        "version": "1.0"
      }
    },
    {
      "url": "https://docs.tigergraph.com/(?P<component>.*?)/current/",
      "variables": {
        "component": [
          "pytigergraph"
        ]
      },
      "extra_attributes":{
        "version": "1.0"
      }
    },
    {
      "url": "https://docs.tigergraph.com/(?P<component>.*?)/",
      "variables": {
        "component": [
          "cloud"
        ]
      },
      "selectors_key": "versionless"
    }
  ],
  "sitemap_urls": [
    "https://docs.tigergraph.com/sitemap.xml"
  ],
  "scrape_start_urls": true,
  "stop_urls": [],
  "selectors": {
    "default": {
      "lvl0": {
        "global": true,
        "selector": ".nav-panel-explore .context .title, .nav-panel-explore .context .version"
      },
      "lvl1": ".doc > h1.page",
      "lvl2": ".doc .sect1 > h2:first-child",
      "lvl3": ".doc .sect2 > h3:first-child",
      "lvl4": ".doc .sect3 > h4:first-child",
      "lvl5": ".doc .sidebarblock > .content > .title",
      "text": ".doc p, .doc dt, .doc td.content, .doc th.tableblock"
    },
    "versionless": {
      "lvl0": {
        "global": true,
        "selector": ".nav-panel-explore .context .title"
      },
      "lvl1": ".doc > h1.page",
      "lvl2": ".doc .sect1 > h2:first-child",
      "lvl3": ".doc .sect2 > h3:first-child",
      "lvl4": ".doc .sect3 > h4:first-child",
      "lvl5": ".doc .sidebarblock > .content > .title",
      "text": ".doc p, .doc dt, .doc td.content, .doc th.tableblock"
    }
  },
  "selectors_exclude": [],
  "min_indexed_level": 1,
  "custom_settings": {
    "advancedSyntax": true,
    "attributesForFaceting": [
      "component",
      "version"
    ],
    "attributesToRetrieve": [
      "anchor",
      "content",
      "hierarchy",
      "url",
      "component",
      "version"
    ],
    "attributesToSnippet": [
      "content:25"
    ],
    "customRanking": [
      "desc(weight.page_rank)",
      "desc(version)",
      "desc(weight.level)",
      "asc(weight.position)"
    ]
  }
}
